page {
  height: 100%;
  background: #b3d4db;
}

.wrapper {
  padding-top: 130rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.avatar {
  border-radius: 100rpx;
  width: 200rpx;
  height: 200rpx;
}

.uname {
  font-size: 32rpx;
  font-weight: bold;
}

.notice {
  margin-top: 140rpx;
  border: solid 1rpx #405f80;
  border-radius: 5rpx;
  text-align: center;
  width: 200rpx;
  height: 80rpx;
}

.start {
  line-height: 80rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #405f80;
}
